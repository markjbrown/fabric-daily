{
  "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/CosmosDB/definition/CosmosDB/1.0.0/schema.json",
  "containers": [
    {
      "id": "SampleData",
      "conflictResolutionPolicy": {
        "mode": "LastWriterWins",
        "conflictResolutionPath": "/_ts",
        "conflictResolutionProcedure": ""
      },
      "geospatialConfig": {
        "type": "Geography"
      },
      "indexingPolicy": {
        "automatic": true,
        "indexingMode": "Consistent",
        "includedPaths": [
          {
            "path": "/*",
            "indexes": []
          }
        ],
        "excludedPaths": [
          {
            "path": "/\"_etag\"/?"
          }
        ],
        "compositeIndexes": [],
        "spatialIndexes": [],
        "vectorIndexes": [],
        "fullTextIndexes": []
      },
      "partitionKey": {
        "paths": [
          "/category"
        ],
        "kind": "Hash",
        "version": 2
      }
    },
    {
      "id": "SampleVectorData",
      "conflictResolutionPolicy": {
        "mode": "LastWriterWins",
        "conflictResolutionPath": "/_ts",
        "conflictResolutionProcedure": ""
      },
      "fullTextPolicy": {
        "defaultLanguage": "en-US",
        "fullTextPaths": []
      },
      "geospatialConfig": {
        "type": "Geography"
      },
      "indexingPolicy": {
        "automatic": true,
        "indexingMode": "Consistent",
        "includedPaths": [
          {
            "path": "/*",
            "indexes": []
          }
        ],
        "excludedPaths": [
          {
            "path": "/\"_etag\"/?"
          },
          {
            "path": "/descriptionVector/*"
          }
        ],
        "compositeIndexes": [],
        "spatialIndexes": [],
        "vectorIndexes": [
          {
            "quantizationByteSize": 64,
            "path": "/descriptionVector",
            "type": "quantizedFlat"
          }
        ],
        "fullTextIndexes": []
      },
      "partitionKey": {
        "paths": [
          "/category"
        ],
        "kind": "Hash",
        "version": 2
      },
      "vectorEmbeddingPolicy": {
        "vectorEmbeddings": [
          {
            "path": "/descriptionVector",
            "dataType": "float32",
            "dimensions": 512,
            "distanceFunction": "cosine"
          }
        ]
      }
    }
  ]
}